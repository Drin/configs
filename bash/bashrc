#environment variables
export BROWSER=chromium

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

#alias definitons
if [ -f ~/.bash_aliases ]; then
   . ~/.bash_aliases
   echo "===> Loaded .bash_aliases";
fi

PS1='[\u@\h \W]\$ '
#/usr/share/java/apache-ant/

PATH=$PATH:.
